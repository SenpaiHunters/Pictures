{
  "manifest_version": 2,
  "name": "SpotOn",
  "version": "5.0.0",
  "description": "Your description here",
  "permissions": ["contextMenus", "notifications", "tabs"],
  "background": {
    "scripts": ["test.js"],
    "persistent": false
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    },
    "default_title": "SpotOn",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"]
}
